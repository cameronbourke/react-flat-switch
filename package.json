{
  "name": "react-flat-switch",
  "version": "1.0.0",
  "description": "Inspired by Oleg Frolov's codepen, React Flat Switch is a toggle/switch component for React.",
  "main": "dist/index.js",
  "keywords": [
    "react",
    "react-component",
    "toggle",
    "switch",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronbourke/react-flat-switch.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base example/ --config webpack.config.js",
    "build": "webpack",
    "compile": "babel src/flat-button.jsx --out-file dist/index.js",
    "prepublish": "npm run compile",
    "test": "tape -r babel-register ./src/**/*.spec.js | tap-spec"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "react-dom": "~0.14.5",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "peerDependencies": {
    "react": "~0.14.7"
  },
  "author": "Cameron Bourke <cameron2295@gmail.com> (http://cameronbourke.com/)",
  "license": "MIT"
}
